    {
        "client_msg_id": "c17181fc-ed4f-4f53-a231-b0301632042b",
        "type": "message",
        "text": "portdelay is a delay after the port is sleep before the link is marked up.",
        "user": "U02DGQYK1",
        "ts": "1544192053.242900"
    },
    {
        "client_msg_id": "c2c1eda4-0c5a-4ba6-8ebe-880eecc59625",
        "type": "message",
        "text": "routedelay is a delay for IPv6 to wait before dhcpv6 is started to allow for router advertisements to show up.",
        "user": "U02DGQYK1",
        "ts": "1544192073.243400",
        "edited": {
            "user": "U02DGQYK1",
            "ts": "1544192080.000000"
