        "edited": {
            "user": "U02DGQYK1",
            "ts": "1544757354.000000"
        }
    },
    {
        "client_msg_id": "ed4dc8b8-7647-440e-911f-9db5249cfac0",
        "type": "message",
        "text": "Debian\/ubuntu installs are annoying.",
        "user": "U02DGQYK1",
