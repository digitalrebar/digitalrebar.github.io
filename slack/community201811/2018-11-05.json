    {
        "client_msg_id": "7a474709-cdb2-453d-b473-5b7ab31f53e1",
        "type": "message",
        "text": "<@UDHGEKL3W> \"remote actions\" relates to the question you are asking about",
        "user": "U02DHRR2L",
        "ts": "1541430159.233000",
        "reactions": [
            {
